<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>jSearch2 Alpha</ShortName>
  <Description>jSearch2 Alpha Plugin</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16" type="image/x-icon">https://search.j-zero.de/favicon.png</Image>
  <Url type="text/html" method="get" template="https://search.j-zero.de/2/">
    <Param name="s" value="{searchTerms}"/>
    <Param name="f" value=""/>
  </Url>
  <Url type="application/x-suggestions+json" template="https://google.com/complete/search?output=firefox&amp;hl=de&amp;q={searchTerms}"/>
  <moz:SearchForm>https://search.j-zero.de/2/</moz:SearchForm>
</OpenSearchDescription>
